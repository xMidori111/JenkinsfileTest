                echo '01 Hello World'
            }
        }
        stage('Build') {
            steps {
                echo '02 Build'
            }
        }
        stage('Deploy') {
            steps {
                echo '03 Deploy'
            }
        }
        stage('Test') {
            steps {
                echo '04 Test'
            }
        }
        stage('Release') {
            steps {
                echo '05 Release'
            }
        }
    }
}
